    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'apfs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=apfs"
                ],
                "filter": {
                    "name": "apfs",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'dump'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=dump",
                    "--package=apfs"
                ],
                "filter": {
                    "name": "dump",
                    "kind": "bin"
                }
            },
            //"args": ["apfs-16k-cs.img"],
            "args": ["testdata/test-apfs.img"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'dump'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=dump",
                    "--package=apfs"
                ],
                "filter": {
                    "name": "dump",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}